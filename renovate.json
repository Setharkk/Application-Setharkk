{
  "extends": ["config:base"],
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "groupName": "all dependencies",
      "automerge": true
    }
  ],
  "ignoreDeps": [],
  "schedule": ["every weekend"],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "rangeStrategy": "bump",
  "semanticCommits": "enabled"
} 